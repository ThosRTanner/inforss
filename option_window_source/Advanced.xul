          </tree>
          <spacer height="5"/>
          <hbox>
            <button label="&inforss.report.refresh;"
                    onclick="Advanced__Report__populate();"/>
          </hbox>
        </vbox>
      </groupbox>
    </vbox>
    <vbox flex="1"> <!-- Debug-->
      <groupbox flex="0">
        <caption label="&inforss.debug;"/>
        <vbox>
          <hbox>
            <label control="debug"
