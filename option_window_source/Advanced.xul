                        orient="horizontal">
              <radio label="&inforss.on;"/>
              <radio label="&inforss.off;"/>
            </radiogroup>
          </hbox>
          <hbox>
            <label control="log"
                   value="&inforss.debug.log;:"
                   tooltiptext="&inforss.debug.log;"
                   class="inforss-label1"
                   flex="0"/>
            <radiogroup id="log"
                        orient="horizontal">
